<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>MMS_chartDynamicLink</name>
        <uuid>8a6ca824-5c2e-47e6-a57a-6ee329b2045c</uuid>
        <description>SAIL interface to create the dynamic link for selecting a month from the chart in the Total Meeting Volume report. To be used with apply()</description>
        <parentUuid>_a-0000dac9-871e-8000-9ba2-011c48011c48_38179</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=#&quot;SYSTEM_SYSRULES_dynamicLink&quot;(&#xD;
  value: ri!monthValue,&#xD;
  saveInto: {&#xD;
    ri!selectedMonth,&#xD;
    a!save(ri!pagingInfo.startIndex, 1)&#xD;
  }&#xD;
)</definition>
        <namedTypedValue>
            <name>monthValue</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>selectedMonth</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>pagingInfo</name>
            <type>
                <name>Variant</name>
                <namespace>http://www.appian.com/ae/types/2009</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>interface</preferredEditor>
    </rule>
